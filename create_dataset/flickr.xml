<?xml version="1.0" encoding="utf-8" ?>
<flickr ver='1.0'>
	<!-- The type can be 'dynamic' or 'fixed' -->
	<time type='dynamic'>
		<!-- format can be 'flickr' or 'unix' -->
		<time_start val='2008-07-31' format='flickr' />
		<time_end val='2015-11-30' format='flickr' />
		<!-- 30 days -->
		<!--time_interval val='2592000' format='unix' /-->
		<!-- 60 days -->
		<time_interval val='2592000' format='unix' />
		<!-- 3 days -->
		<time_interval_min val='259200' format='unix' />
		<time_interval_max val='7776000' format='unix' />
		<time_interval_init val='5184000' format='unix' />
		<!-- Show the overall information every X sec -->
		<time_show val='30' format='unix' />
	</time>
	<fetch maxsize='2000000' batchsize='2000'>
		<page size='500' />
		<labels type='scenes'>
			<label class='0'>portrait</label>
		</labels>
		<labels type='lens'>
			<label class='0'>fisheye</label>
			<label class='0'>ultra wide</label>
			<label class='1'>ultrawide</label>
			<label class='1'>wide angle</label>
			<label class='1'>14mm</label>
			<label class='2'>24mm</label>
			<label class='3'>35mm</label>
			<label class='4'>50mm</label>
			<label class='5'>85mm</label>
			<label class='6'>70-200</label>
			<label class='6'>135mm</label>
			<label class='6'>200mm</label>
			<label class='6'>telephoto</label>
			<label class='6'>canon</label>
			<label class='6'>nikon</label>
			<label class='6'>nikkor</label>
			<label class='6'>sony</label>
		</labels>
		<urls>
			<url>url_n</url>
			<url>url_c</url>
			<url>url_z</url>
		</urls>
	</fetch>
	<hitcontrol>
		<hit_limit>3500</hit_limit>
	</hitcontrol>
	<photoscreen>
