<?xml version="1.0" encoding="utf-8" ?>
<flickr ver='1.0'>
	<auth key='xxxxxxxx' secret='xxxxxxxxxxxx' />
	<!-- The type can be 'dynamic' or 'fixed' -->
	<time type='dynamic'>
		<!-- format can be 'flickr' or 'unix' -->
		<time_start val='2008-01-01' format='flickr' />
		<time_end val='2015-11-30' format='flickr' />
		<!-- 30 days -->
		<time_interval val='2592000' format='unix' />
		<!-- 3 days -->
		<time_interval_min val='259200' format='unix' />
		<time_interval_max val='7776000' format='unix' />
		<time_interval_init val='2592000' format='unix' />
	</time>
	<fetch maxsize='2000000' batchsize='1000'>
		<page size='500' />
		<labels type='scenes'>
			<label class='0'>portrait</label>
			<label class='0'>familyit</label>
			<label class='0'>boy</label>
			<label class='0'>girl</label>
			<label class='1'>indoor</label>
			<label class='1'>living room</label>
			<label class='1'>hail</label>
			<label class='1'>corridor</label>
			<label class='2'>street</label>
			<label class='2'>road</label>
			<label class='2'>building</label>
			<label class='2'>square</label>
			<label class='3'>landview</label>
			<label class='4'>forest</label>
			<label class='4'>beach</label>
			<label class='4'>landscope</label>
			<label class='4'>open country</label>
			<label class='5'>tour</label>
			<label class='5'>travel</label>
			<label class='5'>trip</label>
			<label class='5'>journey</label>
		</labels>
		<labels type='lens'>
			<label class='0'>fisheye</label>
			<label class='1'>ultrawide</label>
			<label class='2'>24mm</label>
			<label class='3'>35mm</label>
			<label class='4'>50mm</label>
			<label class='5'>85mm</label>
			<label class='6'>70-200</label>
			<label class='6'>135mm</label>
			<label class='6'>200mm</label>
		</labels>
		<urls>
			<url>url_n</url>
			<url>url_z</url>
		</urls>
	</fetch>
	<hitcontrol>
		<hit_limit>3600</hit_limit>
	</hitcontrol>
</flickr>
